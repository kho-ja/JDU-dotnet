@model ValidationApp.Models.StudentRegistration

@{
    ViewData["Title"] = "Natija";
}

<h1>Validation natijasi</h1>

<div class="card mt-3">
    <div class="card-body">
        <p><strong>Ism:</strong> @Model.Name</p>
        <p><strong>Familia:</strong> @Model.Surname</p>
        <p><strong>Yosh:</strong> @Model.Age</p>
        <p><strong>Yo'nalish:</strong> @Model.Major</p>
        <p><strong>Email:</strong> @Model.Email</p>
        <p><strong>Student ID:</strong> @Model.StudentId</p>
    </div>
</div>

<a class="btn btn-link mt-3" asp-action="Index">Orqaga</a>
